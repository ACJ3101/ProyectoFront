{
    "version": 2,
    "builds": [
      {
        "src": "angular.json",
        "use": "@vercel/angular"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "https://TU-BACKEND-DOMINIO/api/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  